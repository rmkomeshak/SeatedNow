@{
    ViewData["Title"] = "Home Page";
}
<div class="container-fluid body-content sn-image sn2 fade60" style="color: #fff; padding: 100px 0;">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-lg-12">
                <h2 style="text-align: center; margin-bottom: 20px; font-size: 48px;" class="display-4">What are you craving?</h2>
                <form class="form-inline my-2 my-lg-0" style="max-width: 700px; margin: 0 auto;">
                    <input class="form-control mr-sm-2" type="search" placeholder="Search for a restaurant or keyword" aria-label="Search" style="width: calc(100% - 80px);">
                    <button class="btn btn-sn" type="submit">Search</button>
                </form>
            </div>
        </div>
    </div>
</div>
<div class="container-fluid">
    <div class="container" style="padding-top: 50px; height: 500px;">
        <div class="row">
            <div class="col-lg-12">Hello!</div>
        </div>
    </div>
</div>
