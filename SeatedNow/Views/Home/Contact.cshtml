@{
    ViewData["Title"] = "Contact";
}
<h2>@ViewData["Title"]</h2>
<h3>@ViewData["Message"]</h3>
<head>
    <style>
        #map {
            height: 400px;
            width: 100%;
        }
    </style>
</head>
<body>
    <div>
        <address>
            Seated Now Rd.<br />
            Rochester, MI 48309<br />
            <abbr title="Phone">P:</abbr>
            248-555-5555
        </address>

        <address>
            <strong>Support:</strong> <a href="mailto:Support@seatednow.com">Support@seatednow.com</a><br />
            <strong>Marketing:</strong> <a href="mailto:Marketing@seatednow.com">Marketing@seatednow.com</a>
        </address>
    </div>
    <h3>Seated Now Headquarters</h3>
    <div id="map"></div>
    <script>
        function initMap() {
            var uluru = { lat: 42.677570, lng: -83.219357 };
            var map = new google.maps.Map(document.getElementById('map'), {
                zoom: 16,
                center: uluru
            });
            var marker = new google.maps.Marker({
                position: uluru,
                map: map
            });
        }
    </script>
    <script async defer
            src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAfBylWQ5799KJI6ytzM3_uRYTqWVAphDo&callback=initMap">
    </script>
</body>